<%- include('partials/header'); -%>

<h1>Home</h1>
<p><%= startcontent %></p>
<% for(let i=0;i<items.length;i++){ %>

  <% if(items[i].postname) %>
  <h2 style="display:inline-block;width:900px;color:tomato"><%= items[i].postname %></h2>
  <% if(items[i].aboutpost) {%>
<p><%= items[i].aboutpost.substr(0,150) %>...<a href="/posts/<%= items[i]._id%>">read more</a></p>


  <% }} %>
<%- include('partials/footer'); -%>
